{% extends 'back.html.twig' %}

{% block title %}Sponsor{% endblock %}

{% block body %}
    <h1>Sponsor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sponsor.id }}</td>
            </tr>
            <tr>
                <th>Nom_sponsor</th>
                <td>{{ sponsor.nomSponsor }}</td>
            </tr>
            <tr>
                <th>Description_sponsor</th>
                <td>{{ sponsor.descriptionSponsor }}</td>
            </tr>
            <tr>
                <th>Logo_sponsor</th>
                <td><a  class="custom-logo-link"><img src="{{asset('sponsorImages/'~sponsor.logoSponsor)}}" alt="logo" class="custom-logo"></a></td>
            </tr>
            <tr>
                <th>Site_webs</th>
                   <td><a href="{{ sponsor.siteWebs }}">{{ sponsor.siteWebs }}</a></td>
            </tr>
<tr>
                <th>Nom équipe sponsoriseé</th>
                <td>{{ sponsor.IdEquipe }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sponsorback_index') }}">Retour à la liste</a>

    <a href="{{ path('app_sponsorback_edit', {'id': sponsor.id}) }}">Modifier</a>

    
{% endblock %}
